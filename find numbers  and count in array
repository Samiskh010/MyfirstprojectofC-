#include <iostream>
#include <conio.h>
#include <math.h>
using namespace std;
main()
{
	int a[15]={1,2,3,1,2,3,5,5,5,6,6,6,9,8,9};
	int f[10]={0};
	for(int i=0;i<=14;i++)
	   ++f[a[i]];
	cout<<"data"<<'\t'<<"frequence"<<endl;
	cout<<"*-*-*-*-*-*-*-*-"<<endl;
	for(int j=0;j<=9;j++)
	cout<<j<<'\t'<<f[j]<<endl;
	getch();
}
